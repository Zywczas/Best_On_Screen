<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/DBFragment">


    <fragment
        android:id="@+id/DBFragment"
        android:name="com.zywczas.bestonscreen.views.DBFragment"
        android:label="DBFragment" >
        <action
            android:id="@+id/action_DBFragment_to_apiFragment"
            app:destination="@id/apiFragment" />
        <action
            android:id="@+id/action_DBFragment_to_detailsFragment"
            app:destination="@id/detailsFragment" />
    </fragment>
    <fragment
        android:id="@+id/apiFragment"
        android:name="com.zywczas.bestonscreen.views.ApiFragment"
        android:label="ApiFragment" >
        <action
            android:id="@+id/action_apiFragment_to_detailsFragment"
            app:destination="@id/detailsFragment" />
        <action
            android:id="@+id/action_apiFragment_to_DBFragment"
            app:destination="@id/DBFragment" />
    </fragment>
    <fragment
        android:id="@+id/detailsFragment"
        android:name="com.zywczas.bestonscreen.views.DetailsFragment"
        android:label="DetailsFragment" />


</navigation>